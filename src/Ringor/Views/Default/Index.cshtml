@model Dalion.Ringor.ViewModels.IndexViewModel

@{
  ViewData["Title"] = "Main";
}

@section styles {
  @foreach (var style in Model.Styles) {
    <link rel="stylesheet" type="text/css" href="~/@style" asp-append-version="true"/>
  }
}

<div id="App" class="ui middle aligned center aligned grid"></div>

@foreach (var script in Model.Scripts) {
  <script type="text/javascript" src="~/@script" asp-append-version="true"></script>
}